-- 00

cps(65/120)

let pat1 = "{0*2 ~ [12 ~] ~ 0*4 [~ 0] 0}%8"
    pat2 = "{[~ 0] 0 ~ 0 [~ 0] ~ 0}%8"
    pat3 = "{0 0*4 [~ 0] [~ 0] 0*2 0 ~ 0*4 ~ 0 -12 0 ~ [~ 0] [~ 0] ~ 0*2 -12}%8"
    pat4 = "{0 0*2 [~ 0] [~ 0] 0 0 -12 0*4 ~ 0 ~ 0 ~ [~ 0] [~ 0] ~ 0*2 -12}%8"
    pat5 = "{[~ 0] 0  0*2 ~ [~ 0] [~ 0] ~ 0 -12 0*4 ~ [~ 0] [~ 0] ~ 0*2 -12}%8"
    pat6 = "{~ 0 ~ 0 -12 0*4 0 0*2 0*4 ~ 0 -12 0*2 ~ [~ 0] [~ 0] ~ 0*2 -12}%8"

-- 01

-- solo $
d1
$jux (iter 4)
$s "ikeda1(3, 16, {0, 3})"
#gain "1.5"
#cutoff (scale 800 20000 $rand) #resonance "0.3"
#delay "0.5" #delaytime "0.01 0.02 0.03 0.005" #delayfeedback "0.7"

d2
$s "ikeda2(3,16,{5, 9})"
#sustain "1.0 0.8 1.6"
#gain "1.0"
 
d3
$s "ikeda3(3,16,7)"
#sustain "1.0 0.8 2.0"
#gain "1.0"

-- 02
d4
$jux (iter 16)
$stack[
  s "supersaw(5,16,15)" # n "{c5, e5, f5, g5, a5}" |+| up "12",
  s "supersaw(5,16,10)" # n "{c5, e5, f5, g5, a5}" |+| up "14",
  s "supersaw(5,16,5)" # n "{c5, e5, f5, g5, a5}" |+| up "7",
  s "supersaw(5,16)" # n "{c5, e5, f5, g5, a5}"
]
|+| n "-5 5 0"
#cutoff (scale 2000 15000 $slow 5 $sine1) #resonance "0.3"
#sustain "0.1 0.2"
#gain "0.8"

-- 03

d5
$slow 2
$sound "ifdrums*2 ~ ~ ~ ~ ~ ~ ~"
#n "[0 2] 0 0 0 0 0 0 0"
