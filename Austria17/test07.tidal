-- 05

d6
$every 4 (gap 16)
$jux (iter 4)
$stack[
  --sound "arpy(3,16,3)",
  --sound "felld(3,16,0)",
  sound "fell2(3,16,8)"|+|up "-7 0 7 14",
  sound "fell(2,16,4)",
  sound "bass(2,16,2)",
  sound "dr55(2,16,10)",
  sound "tr-808(2,16,8)",
  sound "casio(2,16,6)",
  sound "glitch(2,16,5)",
  sound "808bd(2,16,9)",
  sound "bd(2,16,3)"
]
#n (irand 128)
#pan (rand)
#up "{7 0 -7 0 5 -12 0 12 0 7 0 -7 0 12}%16"
#cutoff (scale 800 15000 $slow 3 $rand) #resonance "0.3"
